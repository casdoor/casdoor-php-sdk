{
    "name": "casdoor/casdoor-php-sdk",
    "description": "PHP client SDK for Casdoor",
    "authors": [
        {
            "name": "basakest",
            "email": "ab1652759879@gmail.com"
        }
    ],
    "license": "Apache-2.0",
    "keywords": [        
        "casdoor",
        "oauth",
        "saml",
        "authentication",
        "auth",
        "sso",
        "oidc",
        "authn"
    ],
    "require": {
        "php": ">=7.4.0",
        "guzzlehttp/guzzle": "^7.3",
        "league/oauth2-client": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Casdoor\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0|~8.0|~9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Casdoor\\Tests\\": "tests/"
        }
    }
}
